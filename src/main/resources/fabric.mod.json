{
	"schemaVersion": 1,
	"id": "retroapi",
	"version": "${version}",
	"name": "RetroAPI",
	"description": "Simplified block/item registration API for Minecraft b1.7.3",
	"authors": [
		"periut"
	],
	"license": "MIT",
	"environment": "*",
	"entrypoints": {
		"init": [
			"com.periut.retroapi.RetroAPI"
		],
		"stationapi:event_bus": [
			"com.periut.retroapi.compat.StationAPIRegistryForwarder"
		]
	},
	"mixins": [
		"retroapi.mixins.json",
		"retroapi.stationapi.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.17.3",
		"minecraft": "1.0.0-beta.7.3",
		"osl-entrypoints": ">=0.4.0",
		"osl-lifecycle-events": ">=0.5.0"
	},
	"suggests": {
		"stationapi": ">=2.0.0-alpha.5"
	}
}
